    "import pandas as pd\n",
    "from sklearn.tree import DecisionTreeClassifier\n",
    "c_data=pd.read_csv('car_p.csv')\n",
    "x=c_data.drop(columns=['Price'])\n",
    "y=c_data['Price']\n",
    "\n",
    "model = DecisionTreeClassifier()\n",
    "model.fit(x.values,y.values)\n",
    "prediction=model.predict([[200,2.80],[500,4.80]])\n",
    "prediction"
   ]
